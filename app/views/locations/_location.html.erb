<div id="<%= dom_id location %>">
  <p>
    <strong>Address:</strong>
    <%= location.address %>
  </p>

  <p>
    <strong>Lat:</strong>
    <%= location.latitude %>
  </p>

  <p>
    <strong>Long:</strong>
    <%= location.longitude %>
  </p>

  <p>
    <strong>Zipcode:</strong>
    <%= location.zipcode %>
  </p>

</div>
